      <DatePicker fx:id="otField" layoutX="305.0" layoutY="46.0" prefHeight="25.0" prefWidth="107.0" />
      <Button fx:id="btnStatistic" layoutX="206.0" layoutY="361.0" mnemonicParsing="false" onAction="#onStatistic" styleClass="glass" stylesheets="@../css/stylesheet.css" text="Статистика" />
      <Button fx:id="btnCheck" layoutX="305.0" layoutY="361.0" mnemonicParsing="false" onAction="#onCheck" styleClass="glass" stylesheets="@../css/stylesheet.css" text="Сформировать отчет на рейс" />
