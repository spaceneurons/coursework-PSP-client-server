        <Button fx:id="btnFilter" layoutX="519.0" layoutY="46.0" mnemonicParsing="false" onAction="#onFilter" styleClass="glass" stylesheets="@../css/stylesheet.css" text="Фильтровать даты" />
        <Button fx:id="btnSearch" layoutX="193.0" layoutY="46.0" mnemonicParsing="false" onAction="#onSearch" prefHeight="25.0" prefWidth="75.0" styleClass="glass" stylesheets="@../css/stylesheet.css" text="Поиск" />
        <Button fx:id="btnReset" layoutX="534.0" layoutY="361.0" mnemonicParsing="false" onAction="#onReset" styleClass="glass" stylesheets="@../css/stylesheet.css" text="Сбросить" />
