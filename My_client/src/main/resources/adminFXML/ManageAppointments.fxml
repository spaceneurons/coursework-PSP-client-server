                <TableColumn fx:id="start" minWidth="100.0" prefWidth="120.0" text="Вылет" />
                <TableColumn fx:id="end" prefWidth="120.0" text="Сколько лететь" />
                <TableColumn fx:id="service" prefWidth="120.0" text="Рейс" />
                <TableColumn fx:id="client" prefWidth="120.0" text="Турист" />
            </columns>
        </TableView>
        <Button fx:id="btnLogOut" layoutX="543.0" layoutY="2.0" mnemonicParsing="false" onAction="#onLogOut" styleClass="glass" stylesheets="@../css/stylesheet.css" text="Выйти из аккаунта" />
        <Button fx:id="btnBack" layoutX="617.0" layoutY="361.0" mnemonicParsing="false" onAction="#OnBack" prefHeight="25.0" prefWidth="69.0" styleClass="glass" stylesheets="@../css/stylesheet.css" text="Назад" />
