<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="adminbg" stylesheets="@../css/background.css"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="adminFXML.AdminAccount">
   <GridPane layoutX="44.0" layoutY="45.0" prefHeight="311.0" prefWidth="513.0" AnchorPane.bottomAnchor="44.0"
             AnchorPane.leftAnchor="44.0" AnchorPane.rightAnchor="43.0" AnchorPane.topAnchor="45.0">
      <columnConstraints>
         <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
         <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
      </columnConstraints>
      <rowConstraints>
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
      </rowConstraints>
      <Button fx:id="buttonManageUsers" mnemonicParsing="false" onAction="#Manage_Users_Pressed"
              styleClass="glass-grey" stylesheets="@../css/buttons.css" text="Туристы">
         <GridPane.margin>
            <Insets left="15.0" top="10.0"/>
         </GridPane.margin>
      </Button>
      <Button fx:id="btnUsers" mnemonicParsing="false" onAction="#usersPressed" styleClass="glass-grey"
              stylesheets="@../css/buttons.css" text="Пользователи" GridPane.columnIndex="1">
         <GridPane.margin>
            <Insets left="65.0" top="40.0"/>
         </GridPane.margin>
      </Button>
      <Button fx:id="btnEmployees" mnemonicParsing="false" onAction="#Manage_Employees_Pressed"
              styleClass="glass-grey" stylesheets="@../css/buttons.css" text="Персонал" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets left="15.0" top="20.0"/>
         </GridPane.margin>
      </Button>
      <Button fx:id="btnServices" mnemonicParsing="false" onAction="#Manage_Services_Pressed" styleClass="glass-grey"
              stylesheets="@../css/buttons.css" text="Рейсы" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets left="80.0" top="60.0"/>
         </GridPane.margin>
      </Button>
      <Button fx:id="btnAppointments" mnemonicParsing="false" onAction="#Manage_Appointments_Pressed"
              styleClass="glass-grey" stylesheets="@../css/buttons.css" text="Полеты" textFill="BLACK"
              GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets left="15.0" top="30.0"/>
         </GridPane.margin>
      </Button>


      <Button fx:id="btnAddEmplServ" mnemonicParsing="false" onAction="#onAddEmplServ" styleClass="glass"
              stylesheets="@../css/stylesheet.css" text="Назначить рейс" GridPane.columnIndex="1"
              GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets left="50.0" top="60.0"/>
         </GridPane.margin>
      </Button>

   </GridPane>
   <Button fx:id="btnLogOut" layoutX="538.0" layoutY="450.0" mnemonicParsing="false" onAction="#onLogOut"
           styleClass="glass" stylesheets="@../css/stylesheet.css" text="Выход"/>
</AnchorPane>
