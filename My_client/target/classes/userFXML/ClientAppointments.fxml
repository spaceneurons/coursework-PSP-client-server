<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="anchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="bodybg" stylesheets="@../css/background.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="userFXML.ClientAppointments">
    <children>
        <TableView fx:id="appointmentTable" layoutX="4.0" layoutY="52.0" onMouseClicked="#OnMouseClicked" opacity="0.76" prefHeight="282.0" prefWidth="643.0">
            <columns>
                <TableColumn fx:id="id" prefWidth="40.0" text="ID" />
                <TableColumn fx:id="date" minWidth="100.0" prefWidth="120.0" text="Дата" />
                <TableColumn fx:id="start" minWidth="100.0" prefWidth="120.0" text="Начало" />
                <TableColumn fx:id="end" prefWidth="120.0" text="Конец" />
                <TableColumn fx:id="service" prefWidth="120.0" text="Рейс" />
                <TableColumn fx:id="client" prefWidth="120.0" text="Турист" />
            </columns>
        </TableView>
        <Button fx:id="btnLogOut" layoutX="501.0" mnemonicParsing="false" onAction="#onLogOut" styleClass="redline" stylesheets="@../css/stylesheet.css" text="Выйти из аккаунта" />
        <Button fx:id="btnBack" layoutX="574.0" layoutY="361.0" mnemonicParsing="false" onAction="#OnBack" prefHeight="25.0" prefWidth="69.0" styleClass="redline" stylesheets="@../css/stylesheet.css" text="Назад" />
        <Button fx:id="btnAdd" layoutX="14.0" layoutY="361.0" mnemonicParsing="false" onAction="#OnAdd" styleClass="redline" stylesheets="@../css/stylesheet.css" text="Добавить" />
        <Button fx:id="btnEdit" layoutX="106.0" layoutY="361.0" mnemonicParsing="false" onAction="#OnEdit" styleClass="redline" stylesheets="@../css/stylesheet.css" text="Редактировать" />
        <Button fx:id="btnDelete" layoutX="241.0" layoutY="361.0" mnemonicParsing="false" onAction="#OnDelete" styleClass="redline" stylesheets="@../css/stylesheet.css" text="Удалить" />
    </children>
</AnchorPane>
